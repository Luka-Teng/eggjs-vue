@import '//at.alicdn.com/t/font_608463_yr64ohxaqg833di.css'
@import 'http://cdn.w3schools.wang/w3.css'

html
  background-color #fefefe

.img-responsive
  max-width: 100%

.relative
  position: relative

//fade transition
.fade-enter-active
.fade-leave-active
  transition: opacity .5s
  transition: all .5s

.fade-enter
.fade-leave-to
  opacity: 0
  transform: translateY(30px)

.fade-move
  transition: transform 1s
